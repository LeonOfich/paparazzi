<!DOCTYPE module SYSTEM "module.dtd">

<module name="sinus_input">
  <doc>
    <description>Creates a sinus input of varying frequnecy</description>
  </doc>
  <header>
    <file name="sinus_input.h"/>
  </header>
  <init fun="init_sinus_input()"/>
  <periodic fun="periodic_sinus_input()" freq="512" autorun="FALSE" stop = "init_sinus_input()"/>
  <makefile>
    <file name="sinus_input.c"/>
  </makefile>
</module>

